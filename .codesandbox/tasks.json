{
  // These tasks will run in order when initializing your CodeSandbox project.
  "setupTasks": [
    {
      "name": "Install Dependencies",
      "command": "yarn install"
    }
  ],

  // These tasks can be run from CodeSandbox. Running one will open a log in the app.
  "tasks": {
    "start": {
      "name": "start",
      "command": "yarn start",
      "runAtStart": true
    },
    "npm install": {
      "name": "npm install",
      "command": "npm install"
    },
    "npm outdated": {
      "name": "npm outdated",
      "command": "npm outdated"
    },
    "npm install @types/node@latest": {
      "name": "npm install @types/node@latest",
      "command": "npm install @types/node@latest"
    },
    "npm i nodemon": {
      "name": "npm i nodemon",
      "command": "npm i nodemon"
    },
    "npm install express --save": {
      "name": "npm install express --save",
      "command": "npm install express --save"
    },
    "npm watch": {
      "name": "npm watch",
      "command": "npm watch"
    },
    "npm run start": {
      "name": "npm run start",
      "command": "npm run start"
    },
    "npm run watch": {
      "name": "npm run watch",
      "command": "npm run watch"
    },
    "npm i cors": {
      "name": "npm i cors",
      "command": "npm i cors"
    },
    "npm install express-validator": {
      "name": "npm install express-validator",
      "command": "npm install express-validator"
    },
    "npm install prisma": {
      "name": "npm install prisma",
      "command": "npm install prisma"
    },
    "npx prisma init --datasource-provider sqlite": {
      "name": "npx prisma init --datasource-provider sqlite",
      "command": "npx prisma init --datasource-provider sqlite"
    },
    "npx prisma migrate dev --name initial-migration": {
      "name": "npx prisma migrate dev --name initial-migration",
      "command": "npx prisma migrate dev --name initial-migration"
    },
    "npx prisma db seed": {
      "name": "npx prisma db seed",
      "command": "npx prisma db seed"
    },
    "npx prisma seed --reset ./seed.js": {
      "name": "npx prisma seed --reset ./seed.js",
      "command": "npx prisma seed --reset ./seed.js"
    },
    "npx  prisma db seed --reset": {
      "name": "npx  prisma db seed --reset",
      "command": "npx  prisma db seed --reset"
    },
    "npm prisma -h": {
      "name": "npm prisma -h",
      "command": "npm prisma -h"
    },
    "npx prisma migrate reset": {
      "name": "npx prisma migrate reset",
      "command": "npx prisma migrate reset"
    }
  }
}
